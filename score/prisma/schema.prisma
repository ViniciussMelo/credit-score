// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type Items {
  id       Int
  name     String
  price    Float
  quantity Int
}

model Score {
  id     String @id @map("_id")
  userId Int
  email  String
  name   String
  score  Int
}
